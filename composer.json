{
    "name": "erwane/cakephp-i18n-url",
    "description": "Translated URL for CakePHP 3.x",
    "type": "cakephp-plugin",
    "authors": [
        {
            "name": "Erwane Breton",
            "email": "erwane@phea.fr"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "I18nUrl\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "I18nUrl\\Test\\": "tests"
        }
    }
}
